	<ExtendedData><SchemaData schemaUrl="#ds">
		<SimpleData name="cmr_concept_type">CSR+TELLUS+GRACE+Level-3+Monthly+Land+Water-Equivalent-Thickness+Surface+Mass+Anomaly+Release+6.0+version+03+in+netCDF/ASCII/GeoTIFF+Formats</SimpleData>
		<SimpleData name="cmr_concept_id">PODAAC</SimpleData>
